{
  "name": "preact-example",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "TIMING=1 eslint . --ext .js,.ts,.tsx --ignore-path .gitignore",
    "lint:fix": "TIMING=1 eslint . --ext .js,.ts,.tsx --ignore-path .gitignore --fix",
